{
    "manifest_version": 2,
    "name": "tabSleeper",
    "description": "",
    "version": "0.1",
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*" ],
        "js": ["main.js"]
        
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_title": "tabSleeper",
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "background",
        "storage",
        "http://*/*",
        "https://*/*"
    ]
}