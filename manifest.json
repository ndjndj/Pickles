{
    "manifest_version": 2,
    "name": "Pickles",
    "description": "",
    "version": "1.0",
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*" ],
        "js": ["main.js"]
        
    }],
    "browser_action": {
